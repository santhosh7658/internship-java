abstract class Kannan{  
  abstract void family();  
}  
class santhi extends Kannan{
  void family(){System.out.println("santhi is kannan wife");}
}
class santhosh extends Kannan{  
void family(){System.out.println("santhosh is kannan son");}  
}
 
public class Main{
public static void main(String args[]){  
 Kannan obj = new santhosh();   
 Kannan obj1 = new santhi();
 obj.family();
 obj1.family();
}  
} 
